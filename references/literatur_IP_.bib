@unpublished{skottke_versuchsdesign_inhibitionhotspots_2025,
	location = {Kiel},
	title = {Versuchsdesign\_InhibitionHotSpots},
	abstract = {This study investigates the effectiveness of potential inhibitors in preventing corrosion in maritime environments. The tested inhibitors include nickel, aluminum, zinc, tungsten, cobalt, and molybdenum, though only four were selected for laboratory experiments due to storage limitations. The experimental approach involved the cultivation of biofilms to simulate corrosion-promoting conditions, followed by the application of inhibitors in a seawater-steel environment on agar Petri dishes. To prevent galvanic effects, the inhibitors were electrically isolated from the corroding sample. Additionally, a dual test setup was implemented to assess the impact of inhibitors on both pre-corroded and fresh steel samples.  

The study further examined the inhibitors under anaerobic and aerobic conditions to determine variations in effectiveness. Future investigations will focus on defining the inhibitors’ range of effectiveness, their behavior under flowing seawater conditions, solubility, and potential environmental toxicity. The final selection of inhibitors for in-situ experiments is scheduled for May 2025.},
	note = {6. Projekttreffen {MiCorFe} – F\&E Zentrum {FH} Kiel},
	author = {Skottke, Carolin},
	date = {2025-01-21},
	keywords = {{MiCorFe}},
	file = {PDF:/Users/larsmeinke/Zotero/storage/GWFSJ9EJ/Versuchsdesign_InhibitionHotSpots.pdf:application/pdf},
}

@misc{vos_ba_hannah_vos_geschwaerzt_2024,
	title = {{BA}\_Hannah\_Voß\_geschwaerzt},
	abstract = {Im Rahmen des „{MiCorFe}“-Projektes werden neue Beschichtungswerkstoffe und -methoden  entwickelt, die die mikrobielle Korrosion verringern und dadurch den Betrieb und die  Lebensdauer von Offshore-Windenergieanlagen verlängern. Der Einfluss der  Oberflächenbeschaffenheit auf die Korrosion von Baustahl S355 wird in dieser Arbeit untersucht.  Dazu werden insgesamt 72 Proben mit 8 unterschiedlichen Oberflächen präpariert. Die  Präparationen sind as received nur gereinigt, mit einer Feile bearbeitet und geschliffen mit 120,  500, 800, 1200, 2500 und 4000 Körnung. In statischen Laborversuchen werden die Proben für  knapp 3 Monate in künstlichem Meerwasser eingelegt. Zur phänomenologischen Untersuchung  von Korrosionsschutzmaßnahmen werden 24 Proben der 120 und 1200 Körnung mit  handelsüblichem Nagellack und Polyester-Klebeband präpariert. Dazu werden mittig Flächen  abgeklebt bzw. lackiert und Kanten abgeklebt bzw. lackiert. Während der Versuchsdauer erfolgen  an der Hälfte der Proben gravimetrische Vermessungen. Nach Ende der Versuchszeit werden die  Proben unter dem 3D-Mikroskop vermessen. Anschließend werden sie von Korrosionsprodukten  gereinigt durch mechanische Reinigung, ein Ethanolbad im Ultraschall und ein Salzsäurebad.  Zwischen jedem Schritt werden die Proben gravimetrisch vermessen.  Die berechneten Korrosionsraten, die aus dem Masseverlust vor Beginn der Versuche und der  Reinigung mit Salzsäure bestimmt werden, zeigen keinen konkreten Zusammenhang zwischen  der Oberflächenbeschaffenheit und der Korrosionsrate. Die Korrosionsraten befinden sich in  einem Bereich von 0,027 und 0,047 mm/a. Die Korrosionsschutzmaßnahmen mit durchsichtigem  Marken-Nagellack und dem Polyesterklebeband sind positiv zu bewerten und können weiter  verbessert werden. Ein weiteres Ergebnis der Einlegeversuche ist, dass der {pH}-Wert des  künstlichen Meerwassers über die Versuchsdauer von {pH} 7-8 auf {pH} 5,2 gesunken ist. Außerdem  hat das regelmäßige Vermessen der Proben während der Versuchsdauer einen Einfluss auf die  Gewichtsabnahme. Die dauerhaft eingelegten Proben haben weniger Gewicht verloren als die  entnommenen Proben.},
	author = {Voß, Hanna},
	date = {2024-07-15},
	keywords = {Bachelorarbeit {FH} Kiel},
	file = {PDF:/Users/larsmeinke/Zotero/storage/I5BE2PE8/BA_Hannah_Voß_geschwaerzt.pdf:application/pdf},
}

